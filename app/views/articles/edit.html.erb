<h1>Editing Book</h1>
<%= form_for :article, url: article_path, method: :patch do |f| %>
  <%= f.text_field :title %><br>
  <%= f.text_area :text %><br>
  <%= f.submit 'Update Book' %>
<% end %>
<%= link_to 'show',article_path, class: "show_#{@article.id}" %>
<%= link_to 'back', articles_path,class: "back" %>